<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
    <rule>
        <from>/robots.txt</from>
        <to>/robots.txt</to>
    </rule>
    <rule>
        <from>/img/**</from>
        <to>/img/$1</to>
    </rule>
    <rule>
        <from>/html/**</from>
        <to>/html/$1</to>
    </rule>
    <rule>
        <from>/js/**</from>
        <to>/js/$1</to>
    </rule>
    <rule>
        <from>/css/**</from>
        <to>/css/$1</to>
    </rule>
    <rule>
        <from>/img/**.ico</from>
        <to>/img/$1</to>
    </rule>

    <rule>
        <from>/*/img/**</from>
        <to>/$1/img/$2</to>
    </rule>
    <rule>
        <from>/*/html/**</from>
        <to>/$1/html/$2</to>
    </rule>
    <rule>
        <from>/*/js/**</from>
        <to>/$1/js/$2</to>
    </rule>
    <rule>
        <from>/*/css/**</from>
        <to>/$1/css/$2</to>
    </rule>
    <rule>
        <from>/*/img/**.ico</from>
        <to>/$1/img/$2</to>
    </rule>
    <rule>
        <from>/*/images/**.ico</from>
        <to>/$1/images/$2</to>
    </rule>
    <rule>
        <from>/**</from>
        <to>/app/$1</to>
    </rule>
    <outbound-rule>
        <from>/app/**</from>
        <to>/$1</to>
    </outbound-rule>
</urlrewrite>
